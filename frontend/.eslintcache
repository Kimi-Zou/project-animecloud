rt \"./Profile.css\";\n\n// --------------------------------------------------------------------\nconst ProfileTracks = ({ user, track, index }) => {\n  // States\n  const [ wave, setWave ] = useState();\n  const [ play, setPlay ] = useState(false);\n  const [ playProgress, setPlayProgress ] = useState();\n\n  // Player waveform\n  useEffect(() => { \n    const wavesurfer = WaveSurfer.create({\n      container: `#waveform-${index}`,\n      waveColor: '#999',\n      progressColor: '#f50',\n      barHeight: \"1\",\n      barWidth: \"3\",\n      height: \"110\",\n      backend: \"MediaElementWebAudio\"\n    });\n\n    setWave(wavesurfer);\n    \n  }, [index]);\n\n  useEffect(()=>{\n    if(wave) wave.play();\n    if(wave && !play) wave.pause();\n  }, [play])\n  \n\n  // Handle play\n  const clickPlay = () => {\n    setPlay(true);\n  }\n\n  const clickPause = () => {\n\n    setPlayProgress(wave.getCurrentTime()/wave.getDuration());\n    setPlay(false);\n  }\n\n  if(wave) wave.load(`/uploads/tracks/${track.trackPath}`); \n\n  // Calculate track release time\n  const calTime = () => {\n    const currentTime = Date.now();\n    const trackCreatedTime = new Date(track.updatedAt);\n    const timeDifference = (currentTime - trackCreatedTime)/(1000*3600*24);\n    const releaseTime = Math.floor(timeDifference);\n    if(releaseTime < 1) return \"less than 1 day\";\n    return `${releaseTime} days ago`;\n  }\n\n  return (\n    <div className=\"profile-track\">\n      <div className=\"profile-track__cover\">\n        <img className=\"profile-track__cover-img\" src={`/uploads/covers/${track.coverImg}`} alt=\"test\"/>\n      </div>\n      <div className=\"profile-track__music-player\">\n        <div className=\"profile-player__top\">\n          <div className=\"profile-player__top-left\">\n            <div className=\"profile-player__play-icon\">\n              {!play && \n                <i \n                  className=\"fas fa-play-circle fa-4x\" \n                  id={`play-${index}`} \n                  onClick={clickPlay}\n                />\n              }\n              {play && \n                <i \n                  className=\"fas fa-pause-circle fa-4x\" \n                  id={`pause-${index}`} \n                  onClick={clickPause}\n                />\n              }\n              \n            </div>\n            <div className=\"profile-player__name\">\n              <div className=\"profile-player__name-user\">{user.displayName}</div>\n              <div className=\"profile-player__name-track\">{track.title}</div>\n            </div>\n          </div>\n          <div className=\"profile-player__date\">{calTime()}</div>\n        </div>\n        <div className=\"profile-player__middle\" id={`waveform-${index}`}>\n        </div>\n        <div className=\"profile-player__bottom\">\n          <i className=\"fas fa-heart\"></i>\n          <i className=\"fas fa-comment-alt\"></i>\n        </div>\n      </div>\n    </div>\n  )\n}\n\nexport default ProfileTracks;","/Users/kimiris/Desktop/App-Academy/w16 - solo/authenticate-me/frontend/src/components/MusicPlayer/index.js",["104","105"],"/Users/kimiris/Desktop/App-Academy/w16 - solo/authenticate-me/frontend/src/store/player.js",[],{"ruleId":"106","replacedBy":"107"},{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","severity":1,"message":"111","line":11,"column":11,"nodeType":"112","messageId":"113","endLine":11,"endColumn":23},{"ruleId":"114","severity":1,"message":"115","line":32,"column":6,"nodeType":"116","endLine":32,"endColumn":12,"suggestions":"117"},{"ruleId":"114","severity":1,"message":"118","line":98,"column":6,"nodeType":"116","endLine":98,"endColumn":46,"suggestions":"119"},{"ruleId":"114","severity":1,"message":"120","line":98,"column":7,"nodeType":"121","endLine":98,"endColumn":45},"no-native-reassign",["122"],"no-negated-in-lhs",["123"],"no-unused-vars","'playProgress' is assigned a value but never used.","Identifier","unusedVar","react-hooks/exhaustive-deps","React